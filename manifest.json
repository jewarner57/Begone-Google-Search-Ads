{
  "name": "Google Search Ad Blocker.",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Pay to win is bad.",
  "homepage_url": "https://www.github.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "js": [
        "./js/googlesearch.js"
      ],
      "matches": [
        "https://www.google.com/*"
      ]
    }
  ],
  "permissions": [
    "https://www.google.com"
  ]
}